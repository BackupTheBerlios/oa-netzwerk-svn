<?xml version="1.0" encoding="UTF-8"?>
<faces-config xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd" version="2.0">

		<application>
			<locale-config>
				<default-locale>de</default-locale>
				<supported-locale>en</supported-locale>  
			</locale-config>
			<resource-bundle>
				<base-name>messages</base-name>
				<var>msg</var>
			</resource-bundle>
		</application>


		<navigation-rule>
			<from-view-id>/pages/repositories_add.xhtml</from-view-id>
			<navigation-case>
				<from-outcome>success</from-outcome>
				<to-view-id>/pages/overview.xhtml</to-view-id>
				<redirect />
			</navigation-case>
		</navigation-rule>
		<navigation-rule>
			<from-view-id>/pages/repositories_add.xhtml</from-view-id>
			<navigation-case>
				<from-outcome>failed</from-outcome>
				<to-view-id>/pages/repositories_add.xhtml</to-view-id>
			</navigation-case>
		</navigation-rule>
		<navigation-rule>
			<from-view-id>/pages/services_schedule_add.xhtml</from-view-id>
			<navigation-case>
				<from-outcome>failed</from-outcome>
				<to-view-id>/pages/services_schedule_add.xhtml</to-view-id>
			</navigation-case>
		</navigation-rule>
		<navigation-rule>
			<from-view-id>/pages/services_schedule_add.xhtml</from-view-id>
			<navigation-case>
				<from-outcome>success</from-outcome>
				<to-view-id>/pages/services_schedule.xhtml</to-view-id>
			</navigation-case>
		</navigation-rule>

